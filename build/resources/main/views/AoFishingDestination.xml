<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.3.xsd">

    <grid name="ao-destination-grid" title="Destination" model="com.axelor.apps.aoFishing.db.Destination">
        <field name="aoCodeCountry" title="Country Code" min="3" max="3" namecolumn="true"/>
        <field name="aoLibelle" title="Libelle" namecolumn="true"/>
    </grid>


    <form name="ao-destination-form" title="Destination" model="com.axelor.apps.aoFishing.db.Destination">
        <panel name="DestinationPanel" title="Destination">
            <field name="aoCodeCountry" title="Country Code" min="3" max="3" namecolumn="true"/>
            <field name="aoLibelle" title="Libelle" namecolumn="true"/>
        </panel>
    </form>


</object-views>
